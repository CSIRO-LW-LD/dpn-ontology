<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:ds="http://purl.org/dpn/dataset#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/terms/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://purl.org/dpn/dataset">
  <owl:Ontology rdf:about="">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
    <owl:imports rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos"/>
    <owl:imports rdf:resource="http://environment.data.gov.au/water/quality/def/op"/>
  </owl:Ontology>
  <rdfs:Class rdf:ID="TimeInterval">
    <rdfs:subClassOf rdf:resource="http://purl.org/dc/terms/PeriodOfTime"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Time interval</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:ID="Observation">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://environment.data.gov.au/water/quality/def/op#ScaledQuantityKind"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="relatedQuantityKind"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://environment.data.gov.au/water/quality/def/op#SubstanceOrTaxon"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="relatedSubstanceOrTaxon"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="relatedFeature"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </rdfs:Class>
  <owl:Class rdf:ID="Dataset">
    <owl:equivalentClass rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#TimeInterval"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="temporal"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="spatial"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="relatedObservationType"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#Observation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="BoundingBox">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="bbTopLeft"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="bbBottomRight"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Bounding box</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#relatedObservationType">
    <rdfs:range rdf:resource="#Observation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#spatial">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/spatial"/>
    <rdfs:range rdf:resource="#BoundingBox"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >spatial</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#relatedSubstanceOrTaxon">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Related Substance of Taxon that the dataset describes</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#temporal">
    <rdfs:range rdf:resource="#TimeInterval"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >temporal</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#relatedQuantityKind">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Related Quantity Kind that the dataset describes</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#bbBottomRight">
    <rdfs:range rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >bb bottom right</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#bbTopLeft">
    <rdfs:range rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >bb top left</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#relatedFeature">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Related Feature Of Interest that the dataset describes</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="start">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >start</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="end">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >end</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>

